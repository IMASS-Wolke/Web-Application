{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  //Connection for PostgreSQL database
  //Update the prefilled values with your db name and password
  //Ensure you ignore this file in your git repo to avoid exposing sensitive information
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=<DB_NAME>;Username=postgres;Password=<YOUR_PASSWORD>"
  },
  "JWT": {
    "ValidAudience": "http://localhost:5103",
    "ValidIssuer": "http://localhost:5103",
    "Secret": "your-32-character-long-super-strong-jwt-secret-key" //Change for production, Good for Dev
  }
}
